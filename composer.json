{
    "name": "escsrl/jwt-auth",
    "type": "library",
    "description": "Symfony jwt auth bundle. Comes with refresh token enabled by default",
    "keywords": [
        "jwt",
        "auth",
        "symfony-auth"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Alessandro Bellanda",
            "email": "a.bellanda@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Esc\\JwtAuth\\": "src/"
        }
    },
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "escsrl/user": "dev-extensible-user",
        "gesdinet/jwt-refresh-token-bundle": "^0.9",
        "lexik/jwt-authentication-bundle": "^2.6",
        "symfony/http-foundation": "^5.0",
        "symfony/security-bundle": "^5.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/veronicalupopasini/user.git"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "version": "2.0.0"
}
